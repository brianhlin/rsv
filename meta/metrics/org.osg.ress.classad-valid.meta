[org.osg.ress.classad-valid]
default-cron-interval = 24 */2 * * *

execute = local
service-type = OSG-CE
output-format = wlcg
probe-spec = v3


[org.osg.ress.classad-valid env]
PERL5LIB = APPEND | !!VDT_LOCATION!!/osg-rsv/bin/probes
LD_LIBRARY_PATH = APPEND | !!VDT_LOCATION!!/globus/lib
GLOBUS_LOCATION = SET | !!VDT_LOCATION!!/globus
