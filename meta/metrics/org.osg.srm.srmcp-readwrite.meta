[org.osg.srm.srmcp-readwrite]
default-cron-interval = 28 * * * *

execute = local
service-type = OSG-SRM
output-format = wlcg
probe-spec = v3


[org.osg.srm.srmcp-readwrite env]
PERL5LIB = APPEND | !!VDT_LOCATION!!/osg-rsv/bin/probes
LD_LIBRARY_PATH = APPEND | !!VDT_LOCATION!!/globus/lib
GLOBUS_LOCATION = SET | !!VDT_LOCATION!!/globus
