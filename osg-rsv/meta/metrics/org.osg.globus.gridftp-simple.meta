[org.osg.globus.gridftp-simple]
default-cron-interval = 8 * * * *

execute = local
service-type = OSG-GridFtp
output-format = wlcg
probe-spec = v3


[org.osg.globus.gridftp-simple env]
PERL5LIB = PREPEND | !!VDT_LOCATION!!/osg-rsv/bin/probes:!!VDT_PERL5LIB!!
LD_LIBRARY_PATH = APPEND | !!VDT_LOCATION!!/globus/lib
GLOBUS_LOCATION = SET | !!VDT_LOCATION!!/globus
