[org.osg.srm.srmping]
default-cron-interval = 13,33,53 * * * *

execute = local
service-type = OSG-SRM
output-format = wlcg
probe-spec = v3
enable-by-default = true


[org.osg.srm.srmping env]
PERL5LIB = PREPEND | !!VDT_LOCATION!!/osg-rsv/bin/probes:!!VDT_PERL5LIB!!
LD_LIBRARY_PATH = APPEND | !!VDT_LOCATION!!/globus/lib
GLOBUS_LOCATION = SET | !!VDT_LOCATION!!/globus
X509_CERT_DIR = SET | !!VDT_LOCATION!!/globus/TRUSTED_CA
PATH = PREPEND | !!VDT_LOCATION!!/jdk1.6/bin
SRM_JAVA_OPTIONS= SET | -Xms128m -Xmx128m

